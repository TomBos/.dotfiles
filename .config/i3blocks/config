separator=false

[network]
command=$SCRIPT_DIR/network/bandwith
interval=1

[ram]
command=$SCRIPT_DIR/memory/ram
interval=5

[partition home]
command=$SCRIPT_DIR/partitions/home
interval=60

[partition root]
command=$SCRIPT_DIR/partitions/root
interval=60

[time]
command=date '+%d.%m.%Y %H:%M:%S'
interval=5

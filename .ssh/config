
Host github.com
    User git
    Hostname github.com
    IdentitiesOnly yes

Match host github.com exec "test -f ~/.ssh/Archie-GH"
    IdentityFile ~/.ssh/Archie-GH

Match host github.com exec "test -f ~/.ssh/Archie-Laptop-GH"
    IdentityFile ~/.ssh/Archie-Laptop-GH

Match host github.com exec "test -f ~/.ssh/Arch-Work-GH"
    IdentityFile ~/.ssh/Arch-Work-GH



Host bitbucket.org
    User git
    Hostname bitbucket.org
    IdentitiesOnly yes

Match host bitbucket.org exec "test -f ~/.ssh/Arch-Work-IH"
    IdentityFile ~/.ssh/Arch-Work-IH

Match host bitbucket.org exec "test -f ~/.ssh/Archie-Laptop-IH"
    IdentityFile ~/.ssh/Archie-Laptop-IH

Match host bitbucket.org exec "test -f ~/.ssh/Archie-IH"
    IdentityFile ~/.ssh/Archie-IH


